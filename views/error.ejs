<%= layout("/layouts/boilerplate") %>

<% if (typeof err !== 'undefined' && err) { %>
  <div class="row mt-3">
      <div class="alert alert-danger col-6 offset-3" role="alert">
          <p class="alert-heading"><%= err.message %></p>
      </div>
  </div>
<% } else { %>
  <p>No error details available.</p>
<% } %>
